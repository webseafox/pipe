{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithCorrelationIdHeader",
      "WithThreadId",
      "WithClientAgent",
      "WithExceptionDetails"
    ],
    "Properties": {
      "Application": "MiraeDigital.BffMobile.API"
    }
  },
  "AllowedHosts": "*",
  "Proxy": { "TimeOut": "60" },
  "Swagger": {
    "Prefix": "/development/bff-mobile/",
    "Title": "MiraeDigital.BffMobile",
    "Version": [ "v1", "v2" ],
    "XmlFile": "MiraeDigital.BffMobile.API"
  },
